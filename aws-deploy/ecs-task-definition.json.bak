{
  "family": "image-similarity",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "executionRoleArn": "arn:aws:iam::035711439189:role/image-similarity-stack-TaskExecutionRole-BnfykKPkkX0C",
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "BACKEND_IMAGE_URI",
      "cpu": 1024,
      "memory": 2048,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "QDRANT_HOST",
          "value": "host.docker.internal"
        },
        {
          "name": "QDRANT_PORT",
          "value": "6333"
        },
        {
          "name": "REDIS_HOST",
          "value": "host.docker.internal"
        },
        {
          "name": "REDIS_PORT",
          "value": "6379"
        },
        {
          "name": "API_HOST",
          "value": "0.0.0.0"
        },
        {
          "name": "API_PORT",
          "value": "8000"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/image-similarity",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "backend"
        }
      }
    }
  ]
}
